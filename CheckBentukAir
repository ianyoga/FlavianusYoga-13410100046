import java.io.*;
public class CheckBentukAir
{
	public static void main (String[]args) throws IOException
	{
		BufferedReader br=new BufferedReader (new InputStreamReader(System.in));
		System.out.println ("PROGRAM CHECK AIR");
		System.out.println ("Masukkan suhu yang ingin anda check: ");
		int Suhu=Integer.parseInt(br.readLine());
		
		if(Suhu<=0)
		{
			System.out.println ("Air Menjadi padat");
		}
		else if (Suhu>=100)
		{
			System.out.println ("Air Menjadi gas");
		}
		else
		{
			System.out.println ("Air menjadi Cair");
		}
	}
}
